{
    "name": "ts_boilerplate",
    "version": "0.1.0",
    "description": "Quick start for typescript projects",
    "main": "./dist/index.js",
    "author": "Extra <extrahash@protonmail.com>",
    "license": "AGPL-3.0",
    "private": true,
    "scripts": {
        "start": "npx ts-node ./src/index.ts",
        "build": "node ./node_modules/typescript/bin/tsc",
        "format": "node ./node_modules/prettier/bin-prettier.js '**/*.{ts,tsx,json,md}' --tab-width 4 --trailing-comma es5 --ignore-path .gitignore  --arrow-parens always --write",
        "lint": "npx eslint . --ext .ts"
    },
    "devDependencies": {
        "@types/node": "^14.14.25",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "eslint": "^8.14.0",
        "prettier": "^2.2.1",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "axios-retry": "^3.5.1",
        "dotenv": "^8.2.0",
        "electron-log": "^4.4.1",
        "ts-node": "^10.9.1"
    }
}
